{
  "name": "spotify-player-wrapper-tdd-course",
  "version": "1.0.0",
  "description": "A web player the uses spotify-wrapper lib.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage --no-verify",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --watch --require babel-register --reporter=list",
    "test:coverage": "nyc npm test",
    "start": "./node_modules/.bin/webpack-dev-server --open"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://mfpinheiro.com/spotify-player-wrapper-tdd-course"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "mfpinheiro <mateusfpinheiro@gmail.com> (https://github.com/mfpinheiro)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "prettier": "^1.9.1",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
